    If Command1(0).Caption = "EDIT" Then
        Text7(0).Enabled = True
        Text7(0) = Format(Text7(0), "#")
        cmd_simpan
    Else
        simpan
        Text7(0).Enabled = False
    End If
Case 1
    If Command1(1).Caption = "CETAK" Then
        MsgBox "dalam proses", vbOKOnly, "OK deh..."
    Else
        Text7(0).Enabled = False
        cmd_awal
        frmt_text
    End If
Case 2
    Me.Hide
    Akuntansi_frm.Enabled = True
    Akuntansi_frm.Show
End Select
End Sub

